<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net471;netstandard2.0</TargetFrameworks>
    <AssemblyName>Quantumart.QP8.Security</AssemblyName>
    <RootNamespace>Quantumart.QP8.Security</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="netstandard, Version=2.0.0.0, Culture=neutral, PublicKeyToken=cc7b13ffcd2ddd51" />
    <PackageReference Include="NLog" Version="4.5.11" />
  </ItemGroup>

  <PropertyGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
    <DefineConstants>NET_STANDARD</DefineConstants>
  </PropertyGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net471'">
    <Reference Include="System.Web" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="$(SolutionDir)configuration\Configuration.csproj" />
    <ProjectReference Include="$(SolutionDir)constants\Constants.csproj" />
    <ProjectReference Include="$(SolutionDir)utils\Utils.csproj" />
    <ProjectReference Include="$(SolutionDir)\QuantumArt.Infrastructure\QP8.Infrastructure.Logging\QP8.Infrastructure.Logging.csproj" />
    <ProjectReference Include="$(SolutionDir)\QuantumArt.Infrastructure\QP8.Infrastructure\QP8.Infrastructure.csproj" />
  </ItemGroup>
</Project>
