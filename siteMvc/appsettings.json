{
  "Properties": {
    "BackendUrl" : "/Backend",
    "DefaultTheme" : "QP8",
    "UploadMaxSize" : 1024,
    "MaxJsonDeserializerMembers": 5000000,
    "Set500ForHandledExceptions": false,
    "EnableArticleScheduler": true,
    "QpConfigPollingInterval": "00:00:30",
    "EnableSignalR" : true,

    "Globalization" : {
      "DefaultLanguageId" : 1,
      "DefaultCulture": "en-us"
    },

    "Authentication" : {
      "WinLogonUrl": "~/WinLogOn",
      "WinLogonIpRanges" : [
        {
          "Name": "localhost",
          "BeginIp": "127.0.0.1",
          "EndIp": "127.0.0.2"
        }
      ]
    }
  },

  "ArticleScheduler": {
    "RecurrentTimeout": "00:00:30",
    "PrtgLoggerTasksQueueCheckShiftTime": "03:00:00",
    "MailHost": "exchange10"
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  }
}

