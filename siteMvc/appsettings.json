{
  "Properties": {
    "Version": "#{GitVersion.Tag}#.#{GitVersion.CommitsSinceTag}#",
    "BackendUrl" : "/Backend",
    "DefaultTheme" : "QP8",
    "UploadMaxSize" : 1024,
    "MaxJsonDeserializerMembers": 5000000,
    "Set500ForHandledExceptions": false,
    "EnableArticleScheduler": false,
    "EnableCommonScheduler" : false,
    "QpConfigPollingInterval": "00:00:30",
    "EnableSignalR" : false,

    "Globalization" : {
      "DefaultLanguageId" : 1,
      "DefaultCulture": "en-us"
    },

    "Authentication" : {
      "WinLogonUrl": "~/WinLogOn",
      "WinLogonIpRanges" : [
        {
          "Name": "localhost",
          "BeginIp": "127.0.0.1",
          "EndIp": "127.0.0.2"
        }
      ]
    }
  },

  "ArticleScheduler": {
    "RecurrentTimeout": "00:00:30",
    "PrtgLoggerTasksQueueCheckShiftTime": "03:00:00",
    "MailHost": "exchange10"
  },

  "CommonScheduler": {
    "ServiceRepeatInterval": "00:00:30",
    "ServiceRepeatOnErrorInterval": "00:03:00",
    "Tasks": [
      {
        "Name": "Notifications",
        "Interval": "00:00:30"
      },
      {
        "Name": "Cleanup",
        "Interval": "00:10:00"
      }
    ]
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  }
}

