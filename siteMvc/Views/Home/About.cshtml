@using Quantumart.QP8.Resources
@model Quantumart.QP8.WebMvc.ViewModels.HomePage.AboutViewModel

@using (Html.BeginTabForm(Model))
{
    using (Html.BeginFieldGroup(HomeStrings.About))
    {
        @Html.DisplayFieldFor(e => e.Product, "Raw")
        if (Model.IsAdmin)
        {
            @Html.DisplayFieldFor(e => e.DbName)
            @Html.DisplayFieldFor(e => e.DbServerName)
        }
    }
}
